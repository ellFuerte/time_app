> : <Login/> }\n        </Route>\n        <Route path='/Information'>\n          { authUser ? <Information/> : <Login/> }\n        </Route>\n        <Route path='/Reports'>\n          { authUser ? <Reports/> : <Login/> }\n        </Route>\n        <Route path='/*'>\n          <Error/>\n        </Route>\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\nimport {AuthContextProvider} from './context/AuthContext'\nimport './index.css'\n\nReactDOM.render(\n  <React.StrictMode>\n    <AuthContextProvider>\n      <App />\n    </AuthContextProvider>\n  </React.StrictMode>,\n  document.getElementById('root')\n);\n"]}}