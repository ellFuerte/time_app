 default function Register() {\r\n\r\n  const email = useRef()\r\n  const password = useRef()\r\n  const passwordAgain = useRef()\r\n  const username = useRef()\r\n  const department = useRef()\r\n  const [deps, setDeps] = useState([])\r\n  const [error, setError] = useState('')\r\n  const [checkVal, setCheckVal] = useState(false)\r\n  const changeVal = (e) => {\r\n    setCheckVal(!checkVal)\r\n  }\r\n  const history = useHistory()\r\n\r\n  useEffect(() => {\r\n    const getDeps = async () => {\r\n      const res = await axios.get('/api/departments/')\r\n      setDeps(res.data)\r\n    }\r\n\r\n    getDeps()\r\n  }, [])\r\n\r\n  const handleClick = async (e) => {\r\n    e.preventDefault()\r\n    if(password.current.value !== passwordAgain.current.value){\r\n      passwordAgain.current.setCustomValidity('Пароли не совпадают')\r\n    } else if(department.current.value === ''){\r\n      setError('Не все поля заполнены')\r\n      return\r\n    } else {\r\n\r\n      const user = {\r\n        user_name: username.current.value,\r\n        email: email.current.value.toUpperCase(),\r\n        department_id: department.current.value,\r\n        password: password.current.value,\r\n        isadmin: checkVal\r\n      }\r\n      try {\r\n        const res = await axios.post('/api/Register/', user)\r\n        if (res.data.status === \"Username taken\"){\r\n          setError(\"Такой email существует\")}else{\r\n          await axios.post('/api/Register/', user)\r\n          history.push('/login')\r\n        }\r\n      } catch (error) {\r\n      }\r\n\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"register\">\r\n      {error &&\r\n        <div className='error'>\r\n          {error}\r\n        </div>\r\n      }\r\n      <div className=\"registerWrapper\">\r\n        <div className=\"registerCenter\">\r\n          <form className=\"registerBox\" onSubmit={handleClick}>\r\n            <input\r\n              placeholder=\"Фамилия Имя\"\r\n              required ref={username}\r\n              className=\"registerInput\"\r\n            />\r\n            <input\r\n              placeholder=\"Email\"\r\n              type='email'\r\n              required ref={email}\r\n              className=\"registerInput\"\r\n            />\r\n            <select ref={department}>\r\n              <option value=''>Отдел</option>\r\n\r\n              {deps.map((dep, name) =>\r\n\r\n              <option key={name} value={dep.id}>{dep.department_name}</option>)}\r\n\r\n            </select>\r\n            <div>\r\n              Начальник отдела?\r\n              <label htmlFor='trueAdmin'>\r\n              <input\r\n                type='radio'\r\n                value={true}\r\n                name='trueAdmin'\r\n                checked={checkVal}\r\n                onChange={changeVal}\r\n              />Да</label>\r\n              <label htmlFor='falseAdmin'>\r\n              <input\r\n                type='radio'\r\n                value={false}\r\n                name='falseAdmin'\r\n                checked={!checkVal}\r\n                onChange={changeVal}\r\n              />Нет</label>\r\n            </div>\r\n            <input\r\n              placeholder=\"Password\"\r\n              type='password'\r\n              required ref={password}\r\n              className=\"registerInput\"\r\n              minLength='6'\r\n            />\r\n            <input\r\n              placeholder=\"Password Again\"\r\n              type='password'\r\n              required ref={passwordAgain}\r\n              className=\"registerInput\"\r\n              minLength='6'\r\n            />\r\n            <button className=\"registerButton\" type='submit'>Зарегистрироваться</button>\r\n\r\n            <Link to='/login' style={{width:'167%'}}>\r\n\r\n              <button className=\"registerLoginButton\">\r\n                Войти в аккаунт\r\n              </button>\r\n            </Link>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}","import Feed from \"../../components/Feed/Feed\";\r\nimport Sidebar from \"../../components/Sidebar/Sidebar\";\r\nimport Topbar from \"../../components/Topbar/Topbar\";\r\n\r\nimport './Department.css'\r\n\r\nexport default function MainPage() {\r\n  return (\r\n    <>\r\n      <Topbar/>\r\n      <div className=\"mainContainer\">\r\n        <Sidebar/>\r\n        <Feed/>\r\n      </div>\r\n    </>\r\n  )\r\n}","import React from 'react'\r\nimport Topbar from '../../components/Topbar/Topbar'\r\nimport './Information.css'\r\n\r\nexport default function Information() {\r\n  return (\r\n    <>\r\n      <Topbar/>\r\n      <div className='information'>\r\n        <div className=\"informationContainer\">\r\n          <h2 className='infoMainText'>Информация</h2>\r\n          <hr />\r\n          <div className=\"infoContainer\">\r\n            <div className=\"infoDate\">\r\n              10.03.2022\r\n            </div>\r\n            <div className=\"infoText\">\r\n              <h3>Кнопка Начать/Закончить</h3>\r\n              <p>После нажатия на кнопку открывается модальное окно. В нем необходимо выбрать <b>\"здоров\"</b> или <b>\"болен\"</b>. Если выбрано <b>\"болен\"</b>, то нужно дополнительно написать комментарий.</p>\r\n\r\n              <h3>Главная страница</h3>\r\n              <p>На главной странице по умолчанию отображаются все зарегистрированные пользователи. Можно самостоятельно изменить вывод нужных пользователей. Для этого нужно нажать на надпись <b>\"Изменить\"</b> и в модальном окне выбрать необходимое.</p>\r\n\r\n              <h3>Моя страница</h3>\r\n              <p>На данной странице пользователь может изменить свой пароль, для этого надо нажать на надпись <b>\"Изменить пароль\"</b>. В можальном окне нужно ввести старый и новый пароль, если операция прошла успешно, то нужно будет зайти в приложение еще раз.</p>\r\n              <p>Ниже на данной странице есть таблица с данными о работе. Можно дополнительно отфильтровать диапазон нужных дат.</p>\r\n              <p>При нажатии на кнопку <b>\"Отпуск/Больничный\"</b> откроется таблица с данными. Доболнительно появится кнопка <b>\"+\"</b>. При нажатии на нее открывается модальное окно, где можно добавить необходимую информацию. Если текущая дата в ходит в выбранный диапазон, то автоматически меняется <b>статус</b>.</p>\r\n              <h4>Информация для начальников</h4>\r\n              <p>Начальник на странице пользователя дополнительно может изменять информацию, удалять пользователей, сбрасывать пароль и добавлять отпуск/больничный.</p>\r\n\r\n              <h3>Статусы</h3>\r\n              <p style={{marginTop: '10px'}}><b style={{color: 'green'}}>Зеленый</b> - пользователь приступил к работе.</p>\r\n              <p><b style={{color: 'red'}}>Красный</b> - пользователь закончил работу.</p>\r\n              <p><b style={{color: 'orange'}}>Оранжевый</b> - пользователь в отпуске/на больничном.</p>\r\n              <p><b>Белый</b> - пользователь еще не совершал никаких действий.</p>\r\n              <p><b style={{opacity: '0.5'}}>Прозрачный</b> - пользователь удален.</p>\r\n\r\n              <h3>Дерево</h3>\r\n              <p>Через дерево можно переходить на различные отделы. При нажатии на стрелочку появляется список работников этого отдела.</p>\r\n\r\n              <h3>Страница отдела</h3>\r\n              <p>На странице отдела можно увидеть работников отдела и дочерних работников. При нажатии на кнопку <b>\"Сделать главной\"</b> на <b>главной странице</b> появляются работники данного отдела (дополнительно можно выбрать и дочерних работников).</p>\r\n\r\n              <h3>Восстановление пользователя</h3>\r\n              <p>Чтобы восстановить пользователя, нужно зайти на его страницу и поменять статус.</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </>\r\n  )\r\n}\r\n","import { useContext } from \"react\";\nimport {BrowserRouter as Router,\n  Switch,\n  Route,\n  Redirect\n} from \"react-router-dom\";\n\nimport { AuthContext } from \"./context/AuthContext\";\nimport Error from \"./pages/Error/Error\";\nimport Login from \"./pages/Login/Login\";\nimport MainPage from \"./pages/MainPage/MainPage\";\nimport Profile from \"./pages/Profile/Profile\";\nimport Register from \"./pages/Register/Register\";\nimport Department from './pages/Department/Department'\nimport Information from \"./pages/Information/Information\";\n\nfunction App() {\n  const {user} = useContext(AuthContext)\n\n  const userStorage = JSON.parse(localStorage.getItem('user'))\n\n  const authUser = user || userStorage\n\n  return (\n    <Router>\n      <Switch>\n        <Route exact path='/'>\n          { authUser ? <MainPage/> : <Login/> }\n        </Route>\n        <Route path='/login'>\n          { authUser ? <Redirect to='/'/> : <Login/> }\n        </Route>\n        <Route path='/register'>\n          { authUser ? <Redirect to='/'/> : <Register/> }\n        </Route>\n        <Route path='/profile/:username'>\n          { authUser ? <Profile/> : <Login/> }\n        </Route>\n        <Route path='/department/:username'>\n          { authUser ? <Department/> : <Login/> }\n        </Route>\n        <Route path='/information'>\n          { authUser ? <Information/> : <Login/> }\n        </Route>\n        <Route path='/*'>\n          <Error/>\n        </Route>\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\nimport {AuthContextProvider} from './context/AuthContext'\nimport './index.css'\n\nReactDOM.render(\n  <React.StrictMode>\n    <AuthContextProvider>\n      <App />\n    </AuthContextProvider>\n  </React.StrictMode>,\n  document.getElementById('root')\n);\n","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin",