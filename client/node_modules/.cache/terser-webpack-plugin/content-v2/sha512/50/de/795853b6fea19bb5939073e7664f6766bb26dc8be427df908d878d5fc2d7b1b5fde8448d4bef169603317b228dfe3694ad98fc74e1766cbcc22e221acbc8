\" \r\n              type='password' \r\n              className=\"loginInput\" \r\n              ref={password} \r\n              required\r\n              minLength='6'\r\n            />\r\n            <button  className=\"loginButton\" type=\"submit\" disabled={isFetching}>\r\n\r\n              {isFetching ? <CircularProgress style={{color:'white'}}/> : 'Войти'}\r\n\r\n            </button>\r\n\r\n            {/* <span className=\"loginForgot\">Забыли пароль?</span> */}\r\n            <Link to='/register' style={{width:'167%'}}>\r\n              <button className=\"loginRegisterButton\" disabled={isFetching}>\r\n                {isFetching ? <CircularProgress style={{color:'white'}}/> : 'Создать новый аккаунт'}\r\n\r\n              </button>\r\n            </Link>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","import Staff from '../Staff/Staff'\r\nimport './Feed.css'\r\nimport SearchBlock from \"../SearchBlock/SearchBlock\";\r\nimport {useState} from \"react\";\r\n\r\nexport default function Feed() {\r\n  return (\r\n    <div className='feed'>\r\n      <div className=\"feedWrapper\">\r\n        <Staff user={\"user\"} dep={\"depppp\"} func={(xz)=>{console.log(xz)}}/>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n","import { Link, useParams } from 'react-router-dom'\r\nimport { ArrowBack } from '@material-ui/icons'\r\nimport React, { useState } from 'react'\r\n\r\nimport './SidebarDeps.css'\r\nexport default function SidebarDeps({dep, users, setIsShowDeps}) {\r\n  const localUser = JSON.parse(localStorage.getItem('user'))\r\n  const depsUsers = users.filter((user) => dep._id === user.departmentId && user.status !== 4)\r\n  const [isShow, setIsShow] = useState(false)\r\n  const depsId = useParams().username\r\n    console.log(\"depsId=\",dep._id)\r\n  // если выбран департамент\r\n  if(depsId === dep._id){\r\n    dep.backgroundColor = '#e3e3e3'\r\n  }else{\r\n    dep.backgroundColor = ''\r\n  }\r\nconsole.log(\"dep._id=\",deps