от'},'8':{src:'../images/8.png',title:'Птица говорун(голосуем только среди дежурных)'},'9':{src:'../images/9.png',title:'Хома(Голосуем среди коллег из внедрения)'},'10':{src:'../images/10.png',title:'Симка и нолик'}};return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(jsx_runtime[\"Fragment\"],{children:localUser.isAdmin?/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(react_router_dom[\"b\" /* Link */],{to:\"/profile/\".concat(user.id),className:\"name\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:'employee '+findStatus(user.status),children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"angry-grid\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{id:\"item-0\",children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"span\",{className:\"name\",children:[user.user_name,localUser.isAdmin?admin:'']})}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{id:\"item-1\",children:[user.status===5&&/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"vacationText\",children:[\"\\u041E\\u0442\\u043F\\u0443\\u0441\\u043A:\",dateStart,\"-\",dateEnd]}),user.status===6&&/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"vacationText\",children:[\"\\u041E\\u0442\\u0433\\u0443\\u043B:\",dateStart,\"-\",dateEnd]}),user.status===7&&/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"vacationText\",children:[\"\\u0414\\u0440\\u0443\\u0433\\u0430\\u044F \\u043F\\u0440\\u0438\\u0447\\u0438\\u043D\\u0430:\",dateStart,\"-\",dateEnd]}),user.status===3&&/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"vacationText\",children:[\"\\u0411\\u043E\\u043B\\u044C\\u043D\\u0438\\u0447\\u043D\\u044B\\u0439:\",dateStart,\"-\",dateEnd]}),user.status===2&&/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"vacationText\",children:[date,\" \",time]})]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{id:\"item-2\",children:Object.keys(images).map(function(key,id){return(user.nomination_status===